{
  "logreads": true,
  "updpath": ".",
  "killrunpath": "\\kill_run",
  "updmapfilename": "fmap.json",
  "updhttppath": "https://teletek-man.teletek.bg/teletek-man/",
  "paths2inc": [ "ELEMENTS.iris_zones.CONTAINS.IRIS_ZONE", "ELEMENTS.iris_zones.CONTAINS.IRIS4_ZONE", "ELEMENTS.iris_zones.CONTAINS.IRIS8_ZONE", "ELEMENTS.iris_zones.CONTAINS.SIMPO_ZONE", "ELEMENTS.iris_panels_in_network.CONTAINS.IRIS_PANELINNETWORK" ],
  "progress_read_seria_step": 37,
  "progress_read_loop_devs_step": 37,
  "progress_set_seria_step": 11,
  "progress_set_loop_devs_step": 1,
  "progress_write_cmds_step": 53,
  "sleep": {
    "simpo": 7,
    "iris": 10,
    "iris4": 7,
    "iris8": 7
  },
  "inc_fields_on_read": {
    "IRIS8": {
      "IRIS8_INPUT": {
        "P1": 1,
        "P2": 1,
        "~if": "~idx_8=2"
      }
    }
  },
  "iris_login_command": "0660",
  "iris_log_command": "01FF0000",
  "read_replacements": [
    {
      "ELEMENT_ID": "SIMPO_PANELS_R",
      "replacements": [
        {
          "match": "<PROPERTY ID=\"status\" BYTE=\"2\" LENGTH=\"4\"/>",
          "replacewith": "<PROPERTY ID=\"status\" BYTE=\"2\" LENGTH=\"1\"/>"
        }
      ]
    },
    {
      "ELEMENT_ID": "SIMPO_LOGO_R",
      "replacements": [
        {
          "match": "<PROPERTY ID=\"LOGO2\" BYTE=\"85\" LENGTH=\"39\" OPERATION=\"MATRIX_CYR2TEXT\"/>",
          "replacewith": "<PROPERTY ID=\"LOGO2\" BYTE=\"86\" LENGTH=\"39\" OPERATION=\"MATRIX_CYR2TEXT\"/>"
        }
      ]
    },
    {
      "ELEMENT_ID": "SIMPO_LOGO",
      "replacements": [
        {
          "match": "<PROPERTY ID=\"LOGO2\" BYTE=\"92\" LENGTH=\"40\" OPERATION=\"MATRIX_CYR2TEXT\"/>",
          "replacewith": "<PROPERTY ID=\"LOGO2\" BYTE=\"93\" LENGTH=\"40\" OPERATION=\"MATRIX_CYR2TEXT\"/>"
        }
      ]
    },
    {
      "ELEMENT_ID": "SIMPO_PANELS",
      "replacements": [
        {
          "match": "<PROPERTY ID=\"status\" BYTE=\"2\" LENGTH=\"4\"/>",
          "replacewith": "<PROPERTY ID=\"status\" BYTE=\"2\" LENGTH=\"1\"/>"
        }
      ]
    }
  ],
  "write_replacements": [
    {
      "match": "<xxxxBYTE XPATH=\"ELEMENTS/ELEMENT\\[@ID='TFTR_NETWORK'\\]/PROPERTIES/PROPERTY\\[@ID='Name'\\]\" LENGTH=\"82\"/>",
      "replacewith": "<BYTE XPATH=\"ELEMENTS/ELEMENT[@ID='TFTR_NETWORK']/PROPERTIES/PROPERTY[@ID='Name']\" LENGTH=\"83\"/>"
    }
  ]
}