<Window x:Class="ProstePrototype.ScanPopUpWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProstePrototype"
        mc:Ignorable="d"
        ResizeMode="NoResize" WindowStartupLocation="Manual"
        Title="ScanPopUpWindow" Height="150" Width="150"
        ShowInTaskbar="False" Topmost="True" WindowStyle="None">
    <Grid x:Name="grid">
        <!--<WebBrowser x:Name="scanWb" />-->
        <MediaElement x:Name="gifPlayer" LoadedBehavior="Play" UnloadedBehavior="Manual" />
    </Grid>
</Window>
