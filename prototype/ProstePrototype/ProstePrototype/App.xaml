<Application x:Class="ProstePrototype.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ProstePrototype"
             DispatcherUnhandledException="Application_DispatcherUnhandledException"
             StartupUri="Welcome.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="DefaultColor" Color="#0c2b90" />
        <SolidColorBrush x:Key="LighterColor" Color="#007bff" />
        <FontFamily x:Key="fa_brands">/html/imports/webfonts/fa-brands-400.ttf#Font Awesome 6 Brands Regular</FontFamily>
        <FontFamily x:Key="fa_regular">/html/imports/webfonts/fa-regular-400.ttf#Font Awesome 6 Free Regular</FontFamily>
        <FontFamily x:Key="fa_solid">/html/imports/webfonts/fa-solid-900.ttf#Font Awesome 6 Free Solid</FontFamily>
        <FontFamily x:Key="poppins_light">/html/imports/webfonts/Poppins/Poppins-Light.ttf#Poppins Light</FontFamily>
        <FontFamily x:Key="poppins_regular">/html/imports/webfonts/Poppins/Poppins-Regular.ttf#Poppins</FontFamily>
        <FontFamily x:Key="poppins_demibold">/html/imports/webfonts/Poppins/Poppins-SemiBold.ttf#Poppins SemiBold</FontFamily>
        <FontFamily x:Key="poppins_bold">/html/imports/webfonts/Poppins/Poppins-Bold.ttf#Poppins</FontFamily>
        <!--<XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />-->
    </Application.Resources>
</Application>
